{
  "permissions": {
    "allow": [
      "Bash(git --version)",
      "Bash(git init:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(gh repo create:*)",
      "Bash(git remote add:*)",
      "Bash(git branch:*)",
      "Bash(git push:*)",
      "Bash(git config:*)",
      "Bash(gh auth status:*)",
      "Bash(GIT_CURL_VERBOSE=1 GIT_TRACE=1 timeout 30 git push:*)",
      "Bash(ping:*)"
    ]
  }
}
